{
    "compilerOptions": {
        "target": "ES6",
        "moduleResolution": "Node",
        "allowSyntheticDefaultImports": true,
        "baseUrl": ".",
        "checkJs": true,
        "allowJs": true,
        "paths": {
          "@root/*": ["./*"],
          "@constants/*": ["constants/*"],
          "@libs/*": ["./libs/*"],
          "@errors/*": ["./errors/*"],
          "@utils/*": ["./utils/*"],
        }
    },
}